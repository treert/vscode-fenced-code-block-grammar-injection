{
  "name": "codeblock-syntax-highlight",
  "displayName": "OneMore fenced code block syntax highlight",
  "description": "支持比如Markdown的代码块高亮，关联一些特殊的语言",
  "version": "0.0.1",
  "publisher": "onemore",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/treert/vscode-fenced-code-block-grammar-injection.git"
  },
  "bugs": {
    "url": "https://github.com/treert/vscode-fenced-code-block-grammar-injection/issues"
  },
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "grammars": [
			{
				"scopeName": "markdown.fenced.codeblock",
				"path": "./syntaxes/markdown-codeblock.json",
				"injectTo": [
				  "text.html.markdown"
				],
				"embeddedLanguages": {
				  "meta.embedded.block.glsl": "glsl",
				  "meta.embedded.block.hlsl": "hlsl",
				  "meta.embedded.block.cg": "cg",
				  "meta.embedded.block.shaderlab": "shaderlab"
				}
			}
    ]
  },
  "dependencies": {  }
}